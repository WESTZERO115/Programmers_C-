#include <string>
#include <vector>

using namespace std;

int solution(int n, vector<int> lost, vector<int> reserve) {
    int doNotHave = lost.size();
    
    for(int i=0; i<lost.size();i++){
        for(int j=0; j<reserve.size();j++){
            if(reserve[j]==0)
                continue;
            else if(lost[i]-1==reserve[j]){
                lost[i]=0;
                reserve[j]=0;
                doNotHave--;
                continue;
            }
            else if(lost[i]+1==reserve[j]){
                lost[i]=0;
                reserve[j]=0;
                doNotHave--;
                continue;
            }
        }
    }
    
    return (n-doNotHave);
}

//합계: 58.3 / 100.0 실패



