#include <string>
#include <vector>

using namespace std;

string solution(vector<string> seoul) {
    string answer = "김서방은 ";
    
    for(int i= 0; i<seoul.size();i++){
        if(seoul[i]=="Kim"){
           answer += to_string(i);
           answer += "에 있다";
        }
    }
   
    return answer;
}

// ----- 다시 봐야 할 개념 -----
// 1. +=
// 2. to_string(i)
// 자바는 엄청 쉽게 풀었는데 씨쁠쁠은 자바보다는 복잡하다.
